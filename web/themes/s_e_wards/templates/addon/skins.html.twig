<div class="sewards-skins-panel d-xs-none d-sm-none d-md-block d-lg-block d-xl-block">
	<div class="control-panel"><i class="fas fa-eye-dropper"></i></div>
	<div class="sewards-skins-panel-inner">
	   {% include directory ~ '/customize/customize_form.html.twig' %}
	</div>   
</div>

<div class="sewards-skins-panel sewards-skin-demo d-xs-none d-sm-none d-md-block d-lg-block d-xl-block">
	<div class="control-panel"><i class="fa fa-cogs"></i></div>
	<div class="panel-skins-content">
		<div class="title">Color skins</div>
		<div class="text-center">
			<a class="item-color default" href="//{{ current_url }}sews"></a>
			<a class="item-color blue" href="//{{ current_url }}sews=blue"></a>
			<a class="item-color brown" href="//{{ current_url }}sews=brown"></a>
			<a class="item-color green" href="//{{ current_url }}sews=green"></a>
			<a class="item-color lilac" href="//{{ current_url }}sews=lilac"></a>
			<a class="item-color lime_green" href="//{{ current_url }}sews=lime_green"></a>
			<a class="item-color orange" href="//{{ current_url }}sews=orange"></a>
			<a class="item-color pink" href="//{{ current_url }}sews=pink"></a>
			<a class="item-color purple" href="//{{ current_url }}sews=purple"></a>
			<a class="item-color red" href="//{{ current_url }}sews=red"></a>
			<a class="item-color turquoise" href="//{{ current_url }}sews=turquoise"></a>
			<a class="item-color turquoise2" href="//{{ current_url }}sews=turquoise2"></a>
			<a class="item-color violet_red" href="//{{ current_url }}sews=violet_red"></a>
			<a class="item-color violet_red2" href="//{{ current_url }}sews=violet_red2"></a>
